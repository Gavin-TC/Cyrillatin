{
    "manifest_version": 3,
    "name": "Cyrillatin",
    "description": "Allows conversion of latin phonics to cyrillic characters and vice versa.",
    "version": "1.0",

    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "permissions": [
        "contextMenus",
        "tabs"
    ],

    "background": {
        "service_worker": "background.js"
    }
  }